        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "http://hl7.org/v2/StructureDefinition/DTM"
          }
        ],
        "extension": [
          {
            "url": "http://hl7.org/v2/StructureDefinition/optionality",
