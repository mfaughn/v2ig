  "differential": {
    "element": [
      {
        "id": "XAD",
        "path": "XAD",
        "short": "extended address",
        "definition": "*Definition:* This data type specifies the address of a person, place or organization plus associated information.",
        "min": 0,
        "max": "*"
      },
      {
        "id": "XAD.1",
        "path": "XAD.1",
        "short": "Street Address",
        "min": 0,
        "max": "1",
        "type": [
          {
